package parkingLotDesign;
public enum ParkingSpotType{
	HANDICAPPED,COMPACT,LARGE,MOTORBIKE,ELECTRIC
}

public enum VehicleType{
	CAR,TRUCK,ELECTRIC,VAN,MOTORBIKE
}

public enum ParkingStatus{
	OCCUPIED,FREE
}

public enum AccountType{
	CUSTOMER,ATTENDEE
}

